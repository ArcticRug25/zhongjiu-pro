{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["@antfu", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {
    "vue/comment-directive": "off"
  }
}
